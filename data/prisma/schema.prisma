generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}



model Users{
  id_utilizador String  @id @default(uuid())
  nome String  @db.VarChar(255)
  username String  @db.VarChar(255)
  telemovel int
  email String @unique
  password String
  foto
}

model foto{
}

model local{
}

model user {
